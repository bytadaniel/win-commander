{
  "name": "pc-commander",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "bundle": "npx esbuild src/index.ts --bundle --platform=node --outfile=bundle.js",
    "build": "npx tsc -d",
    "pkg:osx": "npm run build && pkg . -t osx -o pc-commander",
    "pkg:win": "npm run build && pkg . -t win -o pc-commander",
    "start": "node build/index.js"
  },
  "pkg": {
    "assets": [
      "scripts/**/*"
    ],
    "targets": [
      "node18"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/winreg": "^1.2.36",
    "@types/wol": "^1.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chardet": "^2.0.0",
    "express": "^4.18.2",
    "getmac": "^6.6.0",
    "nexe": "^4.0.0-rc.4",
    "pkg": "^5.8.1",
    "winreg": "^1.2.5",
    "wol": "^1.0.7"
  }
